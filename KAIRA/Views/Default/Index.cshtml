
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_DefaultLayout.cshtml";
}

@await Component.InvokeAsync("BillBoard")

@await Component.InvokeAsync("Service")

@await Component.InvokeAsync("Category")

@await Component.InvokeAsync("NewArrival")

@await Component.InvokeAsync("Collection")


@await Component.InvokeAsync("BestSelling")

<section class="video py-5 overflow-hidden">
    <div class="container-fluid">
        <div class="row">
            <div class="video-content open-up" data-aos="zoom-out">
                <div class="video-bg">
                    <img src="/kaira/images/video-image.jpg" alt="video" class="video-image img-fluid">
                </div>
                <div class="video-player">
                    <a class="youtube" href="https://www.youtube.com/embed/UpwSrWlKGyg">
                        <svg width="24" height="24" viewBox="0 0 24 24">
                            <use xlink:href="#play"></use>
                        </svg>
                        <img src="/kaira/images/text-pattern.png" alt="pattern" class="text-rotate">
                    </a>
                </div>
            </div>
        </div>
    </div>
</section>

@await Component.InvokeAsync("Testimonial")



@await Component.InvokeAsync("Brand")


@await Component.InvokeAsync("Subscription")

@await Component.InvokeAsync("Gallery")

<footer id="footer" class="mt-5">
    <div class="container">
        <div class="row d-flex flex-wrap justify-content-between py-5">
            @await Component.InvokeAsync("SocialMedia")
            
            @await Component.InvokeAsync("Contact")
            
        </div>
    </div>
</footer>
